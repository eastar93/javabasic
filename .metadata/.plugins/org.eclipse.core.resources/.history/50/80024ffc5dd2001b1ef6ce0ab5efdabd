package Constructor;

public class BasketballPlayer {
	
	public int height;
	public int jump;
	public int dunk;
	
	public BasketballPlayer(int h, int j) 
	{
		h = height;
		j = jump;
	}
	
	public void getInfo()
	{
		System.out.println("키 :" + height);
		System.out.println("점프력 :" +jump);
	}
	
	public void Dunk()
	{
		int dunk = height+jump;
		getInfo();
	}
	
	public void DunkShoot()
	{
		int dunk >= 300;
	}
	
}

