package Garbage;

public class Main {
	
	public static void main(String[] args) {
		
		Item i1 = new Item(1);
		Item i2 = new Item(2);
//		i2 = null;
		Item i3 = new Item(3);
		System.gc();
		//값이 쓰레기인것을 회수하는 명령어.
		Item i4 = new Item(4);
		i4 = null;
		Item i5 = new Item(5);
		System.gc();
		Item i6 = new Item(6);
		
		//스택에 있는 Main의 값이 힙과 연결되어 있으면 쓸모있는 데이터다.
		
	}

}
