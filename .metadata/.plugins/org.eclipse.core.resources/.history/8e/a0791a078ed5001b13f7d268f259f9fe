package Polymorphism.game;

public class Goblin extends Monster {
	
	private String name;
	private int hp;
	private int atk;
	private int def;
	
	public Goblin(String name, int hp, int atk, int def) 
	{
		
		super("Goblin",50,5,3);
		
		this.name=name;
		this.hp=hp;
		this.atk=atk;
		this.def=def;
	}
	
	
}


	
	