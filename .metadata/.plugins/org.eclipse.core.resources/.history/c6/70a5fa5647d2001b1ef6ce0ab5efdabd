package Constructor;

public class Dog {
	
	public String name;
	public int age;
	public String kind;
	public boolean ShelterPuppy;
	
	public Dog(String n, int a, String k, boolean S)
	{
		name = n;
		age = a;
		kind = k;
		ShelterPuppy = S;
		
		System.out.println("이름 :" + name + 
					       ",나이 :" + age + 
					       ",견종 :" + kind + 
					       ",유기견 여부 :" + ShelterPuppy);
	}

}
