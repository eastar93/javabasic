package BuyandSell;

public class Customer02 {
	
	private int money;
	private int watermellon;
	
	public Customer02(int money) {
		this.money = money;
		this.watermellon = 0;
	}
	
	public void buyWatermellon(int w) {
		
		 if((w*2500)>money)
		 {
			 System.out.printf("%d개 만큼 수박을 살 돈이 없습니다.",w);
			 System.out.println("-----------------------");
			 return;
		 }
		 if()
	}

}
