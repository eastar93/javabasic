package Overloading;

public class OverTest {
	private String a;
	private int b;
	
	//main메서드가 없는 경우 java class 파일에는
	//static을 붙이지 않습니다.
	//생성자 문법
	//메서드처럼 작성하지만, 리턴자료형을 기입하지 않고
	//이름은 무조건 해당 클래스 이름과 정확히 같게 만들어줍니다.
	
	public OverTest() {
		this.a = "0";
		this.b = 1;
	}
	public OverTest(String a) {
		System.out.printf("출력1 : ");
	}
	public static void OverTest(String a)
	{
		System.out.printf("출력2 : %s, %d%n", a, 2);
	}
	public static void OverTest(int b)
	{
		System.out.printf("출력3 : %s, %d%n", "3", b);
	}
	public static void OverTest(String a, int b)
	{
		System.out.printf("출력4 : %s, %d%n", a, b);
	}
	public static void main(String[] args) {
		
		OverTest();
		OverTest("G");
		OverTest(3);
		OverTest("A", 3);
	}

}
