package Inheritance.game;

// Commoner 상속을 받게 해주세요
public class Warrior extends Commoner {
	/*
	 * Commoner 상속으로 인해 체력, 마나, 레벨, 아이디 수치는이미 변수로 기입되어 있음
	 * 전사 클래스는 평민과 달리 물리공격력 수치를 부여받게 해 주세요.
	 */
	
	/*
	 * getInfo()함수를 이름 그대로 다시 재정의 합니다.
	 * Warrior의 물리데미지 수치까지 같이 콘솔에 찍어주도록 작성해주세요. 
	 */
	
	public int PDam;
	
	public void getInfo() 
	{
		System.out.printf("ID : %s%n",ID);
		System.out.printf("레벨 : %d%n",Lv);
		System.out.printf("직업 : %s%n",J);
		System.out.printf("체력 : %d%n",Hp);
		System.out.printf("마나 : %d%n",Mp);
		System.out.printf("물리데미지 : %d%n", PDam);
	}


}
